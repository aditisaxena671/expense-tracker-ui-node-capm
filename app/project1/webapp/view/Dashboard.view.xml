<mvc:View
    controllerName="aditi.expense.ui.project1.controller.Dashboard"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:tnt="sap.tnt"
    xmlns:m="sap.m"
    displayBlock="true"
    xmlns:f="sap.f"
>
    <tnt:ToolPage>
        <tnt:header>
            <tnt:ToolHeader>
                <m:Text text="My Dashboard" />
                <f:Avatar initials="AD" />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:sideContent>
            <tnt:SideNavigation>
                <tnt:NavigationList
                selectedKey="dashboard">
                    <tnt:NavigationListItem
                        text="Dashboard"
                        icon="sap-icon://home"
                        press=".onSideNavItemPress"
                        key="dashboard"
                    />
                    <tnt:NavigationListItem
                        text="Add Expense"
                        icon="sap-icon://cart"
                        press=".onSideNavItemPress"
                        key="addExpense"
                    />
                    <tnt:NavigationListItem
                        text="View Analysis"
                        icon="sap-icon://customer"
                        press=".onSideNavItemPress"
                        key="viewAnalysis"
                    />
                    <tnt:NavigationListItem
                        text="View Expense Details"
                        icon="sap-icon://product"
                        press=".onSideNavItemPress"
                        key="expenseDetails"
                    />
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <GenericTile id="firstUpdateOfMonth" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Status Loaded - no press event"
		subheader="Subheader">
		<TileContent unit="Unit" footer="Footer">
			<ImageContent src="sap-icon://money-bills" />
		</TileContent>
	</GenericTile>
	<GenericTile id="lastUpdatedDate" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Last Updated details"
		subheader="Subheader" press="press">
		<TileContent unit="Unit" footer="Footer">
			<ImageContent src="sap-icon://money-bills" />
		</TileContent>
	</GenericTile>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>


